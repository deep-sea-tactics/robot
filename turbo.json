{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".svelte-kit/**", ".vercel/**", ".dist/**"]
    },
    "lint": {},
    "dev": {
      "cache": false,
      "persistent": true,
      "dependsOn": ["^build"]
    },
    "robot": {
      "cache": false,
      "persistent": true
    },
    "robot:mock": {
      "cache": false,
      "persistent": true
    }
  }
}
